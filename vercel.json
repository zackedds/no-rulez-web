{
  "rewrites": [
    { "source": "/api/referee", "destination": "/api/referee" },
    { "source": "/api/opponent", "destination": "/api/opponent" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4",
      "maxDuration": 120
    }
  }
}